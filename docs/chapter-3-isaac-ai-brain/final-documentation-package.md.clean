### Template A: Frame Rate Measurement
```bash
#!/bin/bash
# Systematic Frame Rate Measurement Template
# Educational Purpose: Validate 30+ FPS requirement

echo "Frame Rate Systematic Measurement"
echo "================================="
echo "Date: $(date)"
echo "Target: ‚â•30 FPS continuous"
echo ""

# Measure 1000 frames
echo "Measuring frame rate over 1000 samples..."
start_time=$(date +%s.%N)
frame_count=0
fps_samples=()

while [ $frame_count -lt 1000 ]; do
    current=$(date +%s.%N)
    timestamp=$(echo "$current - $start_time" | bc)
    fps=$(echo "scale=2; $frame_count / $timestamp" | bc)

    fps_samples+=($fps)

    echo "$(($frame_count+1))/1000 - Current FPS: $fps" > /tmp/fps_measurement.log

    ((frame_count++))
    sleep 0.003  # ~30 FPS sampling

    if [ $frame_count -eq 100 ]; then
        echo "100 samples collected..."
    elif [ $frame_count -eq 500 ]; then
        echo "500 samples collected..."
    fi
done

end_time=$(date +%s.%N)
total_time=$(echo "$end_time - $start_time" | bc)
avg_fps=$(echo "scale=2; 1000 / $total_time" | bc)
min_fps=$(printf '%s\n' "${fps_samples[@]}" | sort -n | head -1)
max_fps=$(printf '%s\n' "${fps_samples[@]}" | sort -n | tail -1)

# Calculate 95% confidence interval
std_dev=$(echo "${fps_samples[@]}" | python3 -c "import numpy as np, sys; data=[float(x) for x in sys.stdin.read().split()]; print(np.std(data))")
confidence_interval=$(echo "scale=2; 1.96 * $std_dev / sqrt(1000)" | bc)

echo ""
echo "=== MEASUREMENT RESULTS ==="
echo "Average FPS: $avg_fps"
echo "Min FPS: $min_fps"
echo "Max FPS: $max_fps"
echo "95% Confidence Interval: ¬±$confidence_interval"
echo ""

# SC-003 Validation
if (( $(echo "$avg_fps >= 30.0" | bc -l) )); then
    echo "‚úÖ SC-003 COMPLIANT: Frame rate meets 30+ FPS requirement"
    echo "‚úÖ Student measurement validated successfully"
else
    echo "‚ùå NEEDS IMPROVEMENT: Frame rate below 30 FPS target"
    echo "üìö Learning opportunity: Investigate performance bottlenecks"
fi

echo ""
echo "Student Observable Measurement:"
echo "- Observe frame rate consistency patterns"
echo "- Note relationship between accuracy and frame rate"
echo "- Document optimization techniques for improvement"
echo ""
