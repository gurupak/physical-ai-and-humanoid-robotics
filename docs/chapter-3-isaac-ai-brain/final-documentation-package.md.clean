### Template B: Navigation Accuracy Measurement
```python
#!/usr/bin/env python3
"""
Systematic Navigation Accuracy Measurement Template
Educational validation of 85%+ SC-003 requirement
"""

import numpy as np
from datetime import datetime
import rclpy
from nav_msgs.msg import Path
from geometry_msgs.msg import PoseStamped

class NavigationAccuracyValidator:
    def __init__(self):
        self.target_poses = []
        self.actual_poses = []
        self.measurement_count = 0

    def measure_navigation_accuracy(self, target_pose, actual_pose) -> dict:
        """Systematic measurement of navigation accuracy"""

        # Calculate positional accuracy
        dx = target_pose.position.x - actual_pose.position.x
        dy = target_pose.position.y - actual_pose.position.y
        dz = target_pose.position.z - actual_pose.position.z

        positional_error = np.sqrt(dx**2 + dy**2 + dz**2)

        # Calculate directional accuracy
        target_yaw = self.extract_yaw(target_pose.orientation)
        actual_yaw = self.extract_yaw(actual_pose.orientation)
        angular_error = abs(self.normalize_angle(target_yaw - actual_yaw))

        # Account for humanoid-specific constraints
        footstep_accuracy = self.calculate_footstep_accuracy(actual_pose)
        gait_correlation = self.measure_gait_correlation(actual_pose)

        return {
            'positional_error_m': positional_error,
            'angular_error_rad': angular_error,
            'footstep_accuracy': footstep_accuracy,
            'gait_correlation': gait_correlation,
            'timestamp': datetime.now().isoformat(),
            'student_observation': self.generate_student_notes(positional_error, angular_error)
        }

    def validate_sc003_compliance(self, measurements: list) -> dict:
        """Validate 85%+ SC-003 accuracy requirement"""

        # Calculate comprehensive accuracy
        positional_scores = [m['positional_error_m'] for m in measurements]
        angular_scores = [m['angular_error_rad'] for m in measurements]
        footstep_scores = [m['footstep_accuracy'] for m in measurements]
        gait_scores = [m['gait_correlation'] for m in measurements]

        # Weighted accuracy calculation for humanoids
        overall_accuracy = (
            (85 - np.mean(positional_scores) * 100) * 0.4 +
            (90 - np.mean(angular_scores) * 100) * 0.2 +
            np.mean(footstep_scores) * 0.2 +
            np.mean(gait_scores) * 0.2
        )

        # 95% confidence interval
        std_accuracy = np.std(positional_scores) * 100
        confidence_interval = 1.96 * std_accuracy / np.sqrt(len(measurements))

        return {
            'overall_accuracy': overall_accuracy,
            'confidence_interval': confidence_interval,
            'sc003_compliant': overall_accuracy >= 85.0,
            'enough_measurements': len(measurements) >= 100,
            'student_assessment': self.generate_student_assessment(overall_accuracy)
        }

    def generate_student_notes(self, positional_error: float, angular_error: float) -> str:
        """Generate educational notes for student understanding"""

        notes = []

        if positional_error < 0.1:
            notes.append("Excellent positional accuracy - observe stability factors")
        elif positional_error < 0.2:
            notes.append("Good positional accuracy - note improvement methods")
        else:
            notes.append("Accuracy needs improvement - investigate sensor/data factors")

        if angular_error < 0.05:
            notes.append("Precise directional accuracy achieved")
        else:
            notes.append("Directional accuracy: consider calibration improvements")

        return "; ".join(notes)

# Usage example for systematic validation
if __name__ == '__main__':
    validator = NavigationAccuracyValidator()

    print("Systematic Navigation Accuracy Validation")
    print("========================================\n")

    # Measure 100 navigation commands
    measurements = []

    for i in range(100):
        # Simulate target vs actual navigation
        target_pose = PoseStamped()
        actual_pose = PoseStamped()

        # Add realistic errors for educational demonstration
        target_pose.position.x = 1.0 + i * 0.1
        target_pose.position.y = 0.5 + i * 0.05
        actual_pose.position.x = target_pose.position.x + np.random.normal(0, 0.05)
        actual_pose.position.y = target_pose.position.y + np.random.normal(0, 0.02)

        # Measure accuracy
        result = validator.measure_navigation_accuracy(target_pose.pose, actual_pose.pose)
        measurements.append(result)

        print(f"Measurement {i+1}/100: Accuracy = {85 - result['positional_error_m']*100:.1f}%")

    # Validate SC-003 compliance
    validation = validator.validate_sc003_compliance(measurements)

    print(f"\n=== VALIDATION RESULTS ===")
    print(f"Overall Accuracy: {validation['overall_accuracy']:.1f}% (¬±{validation['confidence_interval']:.1f}%)")
    print(f"SC-003 Compliant: {'‚úÖ YES' if validation['sc003_compliant'] else '‚ùå NO'}")
    print(f"Min Accuracy Required: 85.0%")
    print(f"Student Assessment: {validation['student_assessment']}")
