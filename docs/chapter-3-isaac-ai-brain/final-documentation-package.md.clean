def save_complete_documentation(self, robot_model: str, documentation: Dict) -> str:
    """Save complete documentation to organized structure"""

    output_dir = f"documentation/{robot_model}"
    os.makedirs(output_dir, exist_ok=True)

    # Save each documentation component
    for section, content in documentation.items():
        filename = f"{output_dir}/{section}.md"
        with open(filename, 'w') as f:
            f.write(content)

    # Generate entry index
    index_content = "# " + robot_model + " - Complete Documentation Package\n"
    index_content += "Generated: " + datetime.now().strftime('%Y-%m-%d %H:%M:%S') + "\n\n"
    index_content += "## Documentation Components\n"
    for section in documentation.keys():
        index_content += f"- [{section.title()}]({section}.md)\n"
    index_content += "\n## Quick Access\n"
    index_content += "- üìñ Complete deployment procedures\n"
    index_content += "- üîß Troubleshooting documentation\n"
    index_content += "- üìä Measurement validation guide\n"
    index_content += "- ‚ö° Performance optimization\n"
    index_content += "- üìÖ Maintenance schedules\n"
    index_content += "- üéì Educational framework\n"

    with open(f"{output_dir}/README.md", 'w') as f:
        f.write(index_content)

    return output_dir

